class Task:
    def __init__(self, description, due_date):
        self.description = description
        self.due_date = due_date
        self.completed = False

    def complete(self):
        self.completed = True

class TodoList:
    def __init__(self):
        self.tasks = []

    def add_task(self, task):
        self.tasks.append(task)

    def complete_task(self, index):
        if index < len(self.tasks):
            task = self.tasks[index]
            task.complete()
        else:
            print("Invalid task index.")

    def print_tasks(self):
        print("Todo List:")
        for index, task in enumerate(self.tasks):
            status = "Completed" if task.completed else "Not completed"
            print(f"{index+1}. {task.description} (Due: {task.due_date}) - {status}")

# Example usage:
todo_list = TodoList()

task1 = Task("Buy groceries", "2023-05-30")
task2 = Task("Pay bills", "2023-05-25")
task3 = Task("Finish report", "2023-06-05")

todo_list.add_task(task1)
todo_list.add_task(task2)
todo_list.add_task(task3)

todo_list.complete_task(2)

todo_list.print_tasks()
